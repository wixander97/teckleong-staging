<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_partner_form" model="ir.ui.view">
		<field name="name">	res.partner.form.inherit.teckleong.custom</field>
		<field name="model">res.partner</field>
		<field name="inherit_id" ref="base.view_partner_form" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='category_id']" position="after">
				<field name="fax" />
				<field name="is_driver" />
			</xpath>

			<xpath expr="//form/sheet/group//field[@name='company_id']" position="after">
				<field name="fax" />
			</xpath>
		</field>
	</record>

	<record id="view_partner_tree" model="ir.ui.view">
		<field name="name">	res.partner.tree.inherit.teckleong.custom</field>
		<field name="model">res.partner</field>
		<field name="inherit_id" ref="base.view_partner_tree" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='display_name']" position="after">
				<field name="street" optional="show" />
			</xpath>
		</field>
	</record>
</odoo>