                             <tr>
                                 <td colspan="20" class="no-border" style="font-size: 16px; padding-top: 15px;">
                                     <div class="clearfix">
                                         <div id="total" class="row">
                                             <div class="text-left col-8">
                                                <t t-if="o.narration != False and o.narration != '' and o.narration != '&lt;p&gt;&lt;br&gt;&lt;/p&gt;'">
