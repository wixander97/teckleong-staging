                                                     <t t-foreach="tax_totals['subtotals']" t-as="subtotal">
                                                         <tr style="border-top: 2px solid black !important;">
                                                             <td class="font-weight-bold no-border text-left">
                                                                 Subtotal
                                                             </td>
                                                             <td class="text-right no-border">
                                                                 <span t-att-class="oe_subtotal_footer_separator" t-esc="subtotal['formatted_amount']" />
                                                             </td>
                                                         </tr>
                                                         <tr>
                                                             <td class="font-weight-bold no-border text-left">
                                                                 Overall Discount
                                                             </td>
                                                             <td class="text-right no-border">
                                                                 <t t-esc="int(o.discount_overall)" />%
                                                             </td>
                                                         </tr>
                                                         <t t-set="subtotal_to_show" t-value="subtotal['name']"/>
                                                         <t t-set="subtotal_amaount" t-value="subtotal['amount']"/>
                                                         <t t-call="account.tax_groups_totals"/>
                                                     </t>
                                                     <tr style="border-top: 2px solid black !important;">
                                                         <td class="font-weight-bold no-border text-left">Grand Total</td>
                                                         <td class="text-right no-border">
                                                             <span t-esc="tax_totals['formatted_amount_total']"/>
                                                         </td>
                                                     </tr>

                                                     <t t-if="print_with_payments" style="visibility: hidden;">
                                                         <t t-if="o.payment_state != 'invoicing_legacy'">
