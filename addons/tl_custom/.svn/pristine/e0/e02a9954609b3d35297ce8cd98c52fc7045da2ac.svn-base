<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paperformat_tl_custom_delivery_order" model="report.paperformat">
            <field name="name">Teckleong Delivery Order</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">40</field>
            <field name="margin_bottom">20</field>
            <field name="margin_left">2</field>
            <field name="margin_right">2</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">30</field>
            <field name="dpi">90</field>
        </record>

        <record id="stock.action_report_delivery" model="ir.actions.report">
            <field name="name">Delivery Order</field>
            <field name="paperformat_id" ref="tl_custom.paperformat_tl_custom_delivery_order"/>
        </record>

        <template id="stock.report_delivery_document">
            <t t-call="web.html_container">
                <t t-call="web.basic_layout">
                    <t t-set="o" t-value="o.with_context(lang=o.partner_id.lang)" />
                    <t t-set="partner" t-value="o.partner_id or (o.move_lines and o.move_lines[0].partner_id) or False"/>
                    <div class="header ">
                        <div class="d-flex " style="width: 100%;">
                            <div  style="width:25%;">
                            
                                <img src="/tl_custom/static/img/logoHH.png" style="width:350px;height:165px;"/>
                            </div>
                            <div class="" style="width:50%;text-align:center;">
                                <div class="pt-4"  style="line-height:28px;font-size:26px">
                                    <span style=""><b>HH Stainless Pte Ltd</b></span><br/>
                                    <span class="" style="">32 Gul Crescent, Singapore 629537</span><br/>
                                    <span style="">Telephone: +65 6270 8899, Facsimile : +65 6398 1119</span><br/>
                                    <span style="">www.hhstainles.com.sg, sales@hhstainles.com.sg</span><br/>  
                                    <span style="">Co Reg. No. : 1977004752, GST Reg. No. : M2-0026433-9</span><br/>  
                                </div>   
                            </div>
                            <div class="" style="width:25%;text-align:end;padding-top:25px">                                  
                                <div style="text-transform:uppercase;">
                                    <h2 style="font-size:34px;"><span> delivery order  </span></h2>
                          
                                </div>
                                <div style="margin-top:-10px;font-size:22px;margin-right:30px">
                                    <span>Pg</span>
                                    <span class="page"/> of <span class="topage"/>
                                </div>
                                <div style="text-transform:uppercase;width:100%;">
                                    <div class="d-flex  " style="padding-left:70px;font-size:34px;text-align:right;">
                                        <span class=""  style="font-weight: 600;"><span> Yellow  </span></span>
                                        <span class="pl-1 pr-1" style="border: 1px solid black;">I069411</span>                                                                              
                                    </div>
                                    <div >
                                        <img t-att-src="'/report/barcode/Code128/'+o.origin" style="width:1100px;height:45px;padding-right:650px;" alt="Barcode"/>
                                    </div>
                                   
                                </div>
                                                    
                            </div>
                        </div>
                        
                    
                    </div>    
                    <div class="page ">
                        <!-- <div class="row" style="visibility: hidden;">
                            <div class="col-6" name="div_outgoing_address">
                                <div t-if="o.move_ids_without_package and o.move_ids_without_package[0].partner_id and o.move_ids_without_package[0].partner_id.id != o.partner_id.id">
