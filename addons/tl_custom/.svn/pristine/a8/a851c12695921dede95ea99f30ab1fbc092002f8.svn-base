<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_account_move_inherited_property_tree" model="ir.ui.view">
	    <field name="name">account.move.tree.inherit.teckleong.custom</field>
	    <field name="model">account.move</field>
	    <field name="inherit_id" ref="account.view_out_invoice_tree"/>
	    <field name="arch" type="xml">
	    	<xpath expr="//field[@name='state']" position="after">
	    		<field name="picking_ids" widget="many2many_tags" />
	    	</xpath>
	    </field>
	</record>

	<record id="view_account_move_inherited_property_form" model="ir.ui.view">
	    <field name="name">account.move.form.inherit.teckleong.custom</field>
	    <field name="model">account.move</field>
	    <field name="inherit_id" ref="account.view_move_form"/>
	    <field name="arch" type="xml">
  	      	<xpath expr="//field[@name='user_id']" position="after">
               <field name="po_ref" />
	        </xpath>

			<xpath expr="//div[@name='journal_div']" position="after">
				<field name="discount_overall" />
				<field name="self_collection_location" />
				<field name="is_woocommerce" invisible="1" />
				<field name="picking_ids" widget="many2many_tags" invisible="1" />
		<!-- 		<button type="object" string="aa" name="create_dummy_line" />
				<button type="object" string="post" name="change_state_post" />
				<button type="object" string="draft" name="change_state_draft" />
				<button type="object" string="cancel" name="change_state_cancel" />
				<button type="object" string="paid" name="change_payment_state_paid" /> -->


	        </xpath>

			<xpath expr="//field[@name='invoice_line_ids']/tree//field[@name='quantity']" position="after">
				<field name="discount_overall_id" invisible="1" />
	        </xpath>
			
	        <xpath expr="//field[@name='invoice_line_ids']/tree//field[@name='product_id']" position="after">
	        	<field name="electrode_number" />
	        	<field name="batch_number" />
	      		<field name="custom_order_line_id" invisible="1" />
	        </xpath>

			<xpath expr="//field[@name='line_ids']/tree//field[@name='account_id']" position="after">
				<field name="electrode_number" invisible="1" />
				<field name="batch_number" invisible="1" />
				<field name="custom_order_line_id" invisible="1" />
			</xpath>

	        <xpath expr="//div[@class='o_attachment_preview']" position="replace">
	        	<div class="o_attachment_preview invisible" options="{'preview_priority_type': 'pdf'}"/>
	        </xpath>
	    </field>
	</record>

	<record model="ir.ui.view" id="view_account_invoice_filter">
        <field name="name">account.invoice.select.teckleong.custom</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_account_invoice_filter"/>
	    <field name="arch" type="xml">
	    	<xpath expr="//field[@name='name']" position="after">
	    		<field name="picking_ids"/>
	    	</xpath>
	    </field>
	</record>

	<record id="account.action_move_journal_line" model="ir.actions.act_window">
	    <field name="name">Journal Entries</field>
	    <field name="context">{'default_move_type': 'entry', 'view_no_maturity': True}</field>
	</record>
</odoo>