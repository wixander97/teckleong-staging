                                    <th colspan="5" scope="col">PO Ref</th>
                                    <th colspan="5" scope="col">Cust Ref</th>
                                    <th colspan="3" scope="col">Sales Order</th>
                                    <th colspan="3" scope="col">Invoice</th>
                                </tr>
