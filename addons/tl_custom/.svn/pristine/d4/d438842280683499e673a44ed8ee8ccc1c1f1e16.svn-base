                                    <t t-foreach="lines" t-as="move">
                                        <t t-foreach="move.move_line_ids" t-as="move_line">
                                            <t t-set="counts" t-value="counts+1" />
                                            <tr>
                                                <td colspan="2"><t t-esc="counts"/></td>
                                                <td colspan="5">
                                                    <t t-esc="move_line.product_id.name" />
                                                </td>
                                                <td colspan="6">
                                                    <t t-esc="move_line.product_id.categ_id.name" />
                                                </td>
                                                <td colspan="3">
                                                    <t t-esc="move_line.qty_done" />0 <t t-esc="move_line.product_id.uom_id.name" />
                                                </td>
                                                <td colspan="4">
                                                    <t t-if="move_line.product_id.kg > 0">
                                                        <t t-esc="move_line.product_id.kg * move_line.qty_done" /> kg
                                                        <t t-set="total_weight" t-value="total_weight + move_line.product_id.kg * move_line.qty_done" />
                                                    </t>
                                                    <t t-else="">
                                                        <t t-esc="move_line.product_id.weight * move_line.qty_done" /> kg
                                                        <t t-set="total_weight" t-value="total_weight + move_line.product_id.weight * move_line.qty_done" />
                                                    </t>
                                                </td>
                                            </tr>
                                        </t> 
                                    </t>
                                    <tr>
                                        <td class="text-left" colspan="20">
                                            <b>Total Approx Weight : </b><t t-esc="total_weight" /> kg
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="text-left" colspan="20">
                                            <b>Packing Dimension : </b><t t-esc="packing_dimension" />
                                        </td>
                                    </tr>
                                    <t t-if="o.note != False and o.note != '' and o.note != '&lt;p&gt;&lt;br&gt;&lt;/p&gt;'">
                                        <tr>
                                            <td class="text-left" colspan="20">
                                                <b>Remarks :</b><br />
                                                <span style="line-height: 1.7;" t-field="o.note" />
                                            </td>
                                        </tr>
                                    </t>
                                </tbody>
                            </table>
                        </div>
                        <div class="footer w-100" style="align-item: center;">
                            <!-- <t t-call="tl_custom.cs_footer_received" /> -->
                            <t t-call="tl_custom.cs_footer_custom" />
                        </div>  
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>