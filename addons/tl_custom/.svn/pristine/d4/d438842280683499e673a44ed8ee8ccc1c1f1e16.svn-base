                                        <p class="m-0" t-field="o.teckleong_stock_type"/>
                                    </td> 
                                    <td colspan="3">
                                        <p class="m-0" t-field="o.date_deadline" t-options='{"format": "d/MM/yyyy"}' />
                                    </td>
                                    <td colspan="8">
                                        <t t-if="nani1 == True">
                                            <p class="m-0" t-field="nani1.partner_id.name" />
                                        </t>
                                        <t t-else="">
                                            <t t-esc="o.partner_id.name" />
                                        </t>
                                    </td>
                                    <td colspan="3">
                                        <p class="m-0" t-esc="context_timestamp(datetime.datetime.now()).strftime('%d/%m/%Y')"/>
