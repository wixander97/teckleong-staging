                            <tbody>
                                <t t-set="lines" t-value="o.move_lines.filtered(lambda x: x.product_uom_qty)" />
                                <t t-set="counts" t-value="0" />
                                <t t-foreach="lines" t-as="move">
                                    <t t-set="counts" t-value="counts+1" />
                                    <tr>
                                        <td colspan="1"><t t-esc="counts"/></td>
                                        <td colspan="7" class="text-left">
                                            <t t-esc="move.description_picking"/>
                                        </td>
                                        <td colspan="5"><span t-field="move.product_id.categ_id.name" t-options="{'widget': 'text'}"/></td>
                                        <td colspan="3">
                                            <t t-if="move.electrode_number != False and move.electrode_number != ''">
                                                <t t-esc="move.electrode_number" />/<br />
                                            </t>
                                            <t t-foreach="move.move_line_ids" t-as="lot">
                                                <t t-esc="lot.lot_id.name" />
                                                <t t-if="lot != move.move_line_ids[-1]">
                                                    ;
                                                </t>
                                            </t>
                                        </td>
                                        <td colspan="4"><span t-field="move.product_uom_qty" /> <span t-field="move.product_id.uom_id.name" /></td>
                                    </tr>
                                    <t t-if="move.notes">
                                        <tr>
                                            <td class="text-left" name="td_notes" colspan="20">
                                                <b>Notes : </b><t t-esc="move.notes" />
                                            </td>
                                        </tr>
                                    </t>
                                </t>
                                <t t-if="o.note != False and o.note != '' and o.note != '&lt;p&gt;&lt;br&gt;&lt;/p&gt;'">
                                    <tr>
                                        <td class="text-left" colspan="20">
                                            <b>Remarks :</b><br />
                                            <span style="line-height: 1.7;" t-field="o.note" />
                                        </td>
                                    </tr>
                                </t>
                            </tbody>
