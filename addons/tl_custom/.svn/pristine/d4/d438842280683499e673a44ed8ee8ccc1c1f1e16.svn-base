                                    <th colspan="3" scope="col">Payment Terms</th>
                                    <th colspan="3" scope="col">Shipping Terms</th>
                                    <th colspan="3" scope="col">Date Required</th>
                                    <th colspan="8" scope="col">Ordered By</th>
                                    <th colspan="3" scope="col">Date</th>
                                </tr>
                                <tr>
                                    <td colspan="3">
                                        <t t-set="ceks" t-value="o.origin" />
                                        <t t-set="nani1" t-value="False" />
                                        <t t-if="ceks">
                                            <t t-if="ceks[0] == 'S'">
                                                <t t-set="idso" t-value="request.env['sale.order']" />
                                            </t>
                                            <t t-else="">
                                                <t t-set="idso" t-value="request.env['purchase.order']" />
                                            </t>
                                            <t t-set="nani1" t-value="idso.search([('name', '=',o.origin )],limit=1)" />     
                                            NET <t t-esc="nani1.payment_term_id.name"/>
                                        </t>
                                        <t t-else="">
                                            -
                                        </t>
                                    </td> 
