                                    <t t-set="rdoc" t-value="request.env['account.move']" />
                                    <t t-if="o.origin == True">
                                        <t t-set="rdo1" t-value="rdoc.search([('invoice_origin', '=',o.origin), ('state', 'not in', ['cancel', 'draft'])])" />  
                                    </t>
                                    <td colspan="4">
                                        <!-- <p class="m-0" t-field="rdo1.user_id.name"/> -->
                                        <t t-if="o.sale_order_id">
                                            <p class="m-0" t-field="o.sale_order_id.user_id.name"/>
                                        </t>
                                        <t t-else="">
                                            -
                                        </t>
                                    </td>
                                    <td colspan="5">
                                        <t t-if="o.po_ref">
                                            <p class="m-0" t-field="o.po_ref"/>
                                        </t>
                                        <t t-else="">
                                            -
                                        </t>
                                    </td>
                                    <td colspan="5">
                                        <t t-if="o.customer_ref">
                                            <p class="m-0" t-field="o.customer_ref"/>
                                        </t>
                                        <t t-else="">
                                            -
                                        </t>
                                    </td>
                                    <td colspan="3">
                                        <t t-if="o.origin">
                                            <p class="m-0" t-field="o.origin"/>
                                        </t>
                                        <t t-else="">
                                            -
                                        </t>
                                    </td>
